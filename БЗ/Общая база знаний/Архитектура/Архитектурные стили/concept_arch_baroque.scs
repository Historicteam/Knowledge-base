	...
	(*
	<-sc_explanation;;
	<=nrel_sc_text_translation: 
		...
		(*
		-> rrel_example:
		[Для архитектуры барокко характерна определенная художественная свобода и экспериментаторство, в отличие от строгого греческого возрождения середины XVII века.]
			(*
			<- lang_ru;;
			*);;

		*);;
*);

