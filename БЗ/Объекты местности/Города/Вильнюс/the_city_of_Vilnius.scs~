the_city_of_Vilnius
<-sc_const;
=> nrel_main_idtf:
	[Вильнюс]
	(*
	<- lang_ru;;
	*);
	[Vilnius]
	(*
	<- lang_en;;
	*);
=> nrel_idtf:
	[Вильно]
	(*
	<- lang_ru;;
	*);
	[Вильна]
	(*
	<- lang_ru;;
	*);
	[Wilno]
	(*
	<- lang_en;;
	*);
	[Vilna]
	(*
	<- lang_en;;
	*);

=>nrel_coat_of_arms:coat_of_arms_of_Vilnius;

<-concept_city;

<- rrel_key_sc_element:
	Definition_of_the_concept_of_Vilnius
	(*
	=> nrel_main_idtf:
		[Определение понятия "Вильнюс"](* <- lang_ru;; *);
		[Definition of the concept of Vilnius](* <- lang_en;; *);;
	<-sc_definition;;
	<=nrel_sc_text_translation:
		...
		(*
		->rrel_example:
			[Вильнюс —  город в Польше, на левом берегу Вислы, второй по населению город Польши после Варшавы, ненамного опережает Лодзь, административный центр Малопольского воеводства, относится к историко-географической области Галиция и в историческом плане является одной из её столиц, центр города занесён в список всемирного наследия ЮНЕСКО, один из крупнейших научных, культурных и экономических центров Польши, популярное место туризма.]
			(*
			<- lang_ru;;
			*);;
		*);;
*);

